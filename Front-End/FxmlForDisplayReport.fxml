<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="461.0" prefWidth="696.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SemesterProject.ControllerForDisplayReport">
   <children>
      <TableView fx:id="reportsTable" layoutX="3.0" layoutY="115.0" prefHeight="340.0" prefWidth="696.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="75.0" text="NAME" />
          <TableColumn fx:id="ageColumn" prefWidth="75.0" text="AGE" />
            <TableColumn fx:id="cnicColumn" prefWidth="86.0" text="CNIC" />
            <TableColumn fx:id="residenceColumn" prefWidth="96.0" text="RESIDENCE" />
            <TableColumn fx:id="crimeColumn" prefWidth="105.0" text="CRIME REPORTED" />
            <TableColumn fx:id="reportTimeColumn" prefWidth="116.0" text="REPORT TIME" />
            <TableColumn fx:id="caseId" prefWidth="75.0" text="CASE ID" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="goBack" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onGoBackButtonClicked" text="&lt;=">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Label layoutX="242.0" layoutY="44.0" text="Displaying Reports">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
